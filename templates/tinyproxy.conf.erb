# Managed by puppet , do not edit it !!

Listen <%= @listen %>
<% if @bind -%>
Bind <%= @bind %>
<% end -%>
<% if @bind_same -%>
BindSame <%= @bind_same %>
<% end -%>
<% @allow.each do |allow_host| -%>
Allow <%= allow_host %>
<% end -%>
<% if @deny -%>
<% @deny.each do |deny_host| -%>
Deny <%= deny_host %>
<% end -%>
<% end -%>
ConnectPort <%= @connect_port %>
<% if @error_files -%>
<% @error_files.each_pair do |code, location| -%>
ErrorFile <%= code %> "<%= location %>"
<% end -%>
<% end -%>
DefaultErrorFile "<%= @default_error_file %>"
<% if @stat_host -%>
StatHost "<%= @stat_host %>"
<% end -%>
<% if @stat_file -%>
StatFile "<%= @stat_file %>"
<% end -%>
<% if @use_syslog -%>
Syslog Yes
<% end -%>
<% if @use_xtinyproxy -%>
XTinyproxy On
<% end -%>
<% if @filter_default_deny -%>
FilterDefaultDeny Yes
<% end -%>
<% if @filter_file -%>
Filter "<%= @filter_file %>"
<% end -%>
<% if @filter_extended -%>
FilterExtended On
<% end -%>
User <%= @user %>
Group <%= @group %>
Logfile "<%= @log_file %>"
LogLevel <%= @log_level %>
MaxClients <%= @max_clients %>
MaxRequestsPerChild <%= @max_requests_perchild %>
MaxSpareServers <%= @max_spare_servers %>
MinSpareServers <%= @min_spare_servers %>
PidFile "<%= @pid_file %>"
Port <%= @port %>
StartServers <%= @start_servers %>
Timeout <%= @timeout %>
<% if @add_headers -%>
<% @add_headers.each_pair do |header, value| -%>
AddHeader <%= header %> "<%= value %>"
<% end -%>
<% end -%>
<% if @via_proxy_name -%>
ViaProxyName "<%= @via_proxy_name %>"
<% end -%>
<% if @disable_via_header -%>
DisableViaHeader Yes
<% end -%>